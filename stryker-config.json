{
  "stryker-config": {
    "concurrency": 8,
    "mutation-level": "Standard",
    "language-version": "latest",
    "additional-timeout": 5000,

    "project": "src/FitnessBooking.Application/FitnessBooking.Application.csproj",

    "mutate": [
      "src/FitnessBooking.Application/**/*.cs"
    ],

    "coverage-analysis": "perTest",

    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },

    "reporters": [
      "progress",
      "html"
    ],

    "test-projects": [
      "tests/FitnessBooking.UnitTests/FitnessBooking.UnitTests.csproj",
      "tests/FitnessBooking.PropertyTests/FitnessBooking.PropertyTests.csproj",
      "tests/FitnessBooking.IntegrationTests/FitnessBooking.IntegrationTests.csproj"
    ],

    "test-case-filter": "",
    "ignore-mutations": [],
    "ignore-methods": [],
    "report-file-name": "mutation-report",
    "break-on-initial-test-failure": false
  }
}
