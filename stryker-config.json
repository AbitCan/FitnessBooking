{
  "stryker-config": {
    "solution": "FitnessBooking.sln",
    "test-projects": [
      "tests/FitnessBooking.UnitTests/FitnessBooking.UnitTests.csproj"
    ],
    "project": "src/FitnessBooking.Application/FitnessBooking.Application.csproj",
    "mutate": [
      "src/FitnessBooking.Application/**/*.cs"
    ],
    "coverage-analysis": "perTest",
    "reporters": [
      "progress",
      "html"
    ],
    "concurrency": 4,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "verbosity": "info"
  }
}
